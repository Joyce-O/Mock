        <div class="views-row views-row-1">
      
  <div class="views-field views-field-title">        <h1 class="field-content page__title">Nutrition Society Live 2020</h1>  </div>  
  <div class="views-field views-field-field-image">        <div class="field-content"><img typeof="foaf:Image" src="https://www.nutritionsociety.org/sites/default/files/styles/simplecrop__1200_x_400/public/images/event/nslive_cover_photo_2.0.jpg?=QekII9BB&amp;sc=45a9362a45f6829bdd88984dceba7e6f" width="1200" height="400" alt="Nutrition Society Live 2020" /></div>  </div>  
  <span class="views-field views-field-nothing">        <span class="field-content"><div class="block-event-header__info"></span>  </span>  
  <span class="views-field views-field-field-location-name">        <span class="field-content">Online Event</span>  </span>  
  <span class="views-field views-field-field-date">        <span class="field-content"><div class="date-display-range"><span 
        class="date-display-start" property="dc:date" 
        datatype="xsd:dateTime" content="2020-07-14BST09:00:00+01:00">14</span> - <span class="date-display-end" property="dc:date" datatype="xsd:dateTime" content="2020-07-15BST17:00:00+00:00">15 July 2020</span></div></span>  </span>  
  <div class="views-field views-field-views-conditional">        <span class="field-content"><a href="https://www.nutritionsociety.org/civicrm/event/register?id=112&reset=1">Register Now</a></span>  </div>  
  <span class="views-field views-field-nothing-1">        <span class="field-content"></div></span>  </span>  </div>
